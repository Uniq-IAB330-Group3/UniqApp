<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Uniq.Views.AppointmentPage" Title="{Binding Title}">
    <StackLayout Spacing="20" Padding="15">
        <Entry Placeholder="Due Date" />
        <Entry Placeholder="Time" />
        <Entry Placeholder="Reason for Extension" />
        <Button Text="Submit"/>
    </StackLayout>
</ContentPage>